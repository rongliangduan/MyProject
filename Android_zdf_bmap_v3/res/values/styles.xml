<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.    -->


    <!-- Application theme. -->
    <style name="AppTheme" parent="Theme.Sherlock.Light">

        <!-- window transparent -->
        <!--
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        -->

        <!-- Include block: NoActionBar -->
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <!-- End of block NoActionBar -->
    </style>

    <style name="MeizuAbsTheme" parent="Theme.Sherlock">
        <item name="windowActionBarOverlay">true</item>
        <item name="android:windowActionBarOverlay">true</item>

        <!--
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        -->

        <item name="android:windowContentOverlay">@null</item>
        <item name="actionBarStyle">@style/CustomActionBarStyle</item>
        <item name="android:actionBarStyle">@style/CustomActionBarStyle</item>
        <item name="actionButtonStyle">@style/CustomActionButtonStyle</item>
        <item name="android:actionButtonStyle">@style/CustomActionButtonStyle</item>
        <item name="searchViewSearchIcon">@drawable/ic_search</item>
        <item name="actionBarItemBackground">@color/transparent</item>
        <item name="android:actionBarItemBackground">@color/transparent</item>
    </style>

    <style name="NormalAbsTheme" parent="Theme.Sherlock">
        <item name="windowActionBarOverlay">false</item>
        <item name="android:windowActionBarOverlay">false</item>

        <!--
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        -->

        <item name="android:windowContentOverlay">@color/black</item>
        <item name="actionBarStyle">@style/CustomActionBarStyle</item>
        <item name="android:actionBarStyle">@style/CustomActionBarStyle</item>
        <item name="actionButtonStyle">@style/CustomActionButtonStyle</item>
        <item name="android:actionButtonStyle">@style/CustomActionButtonStyle</item>
        <item name="searchViewSearchIcon">@drawable/ic_search</item>
        <item name="actionBarItemBackground">@color/transparent</item>
        <item name="android:actionBarItemBackground">@color/transparent</item>
    </style>

    <style name="Holo.Theme.CustomAbsOverlay" parent="Theme.Sherlock">
        <item name="windowActionBarOverlay">false</item>
        <item name="android:windowActionBarOverlay">false</item>
        <item name="actionBarItemBackground">@color/transparent</item>
        <item name="android:actionBarItemBackground">@color/transparent</item>
        <item name="android:actionBarStyle">@style/CustomAbsOverlayStyle</item>
    </style>

    <style name="CustomActionButtonStyle" parent="Widget.Sherlock.ActionButton">
        <item name="android:paddingLeft">0dip</item>
        <item name="android:paddingStart">0dip</item>
        <item name="android:paddingRight">0dip</item>
    </style>

    <style name="CustomActionBarStyle" parent="Widget.Sherlock.ActionBar.Solid">
        <item name="titleTextStyle">@style/tv_large_white</item>
        <item name="android:titleTextStyle">@style/tv_large_white</item>
        <item name="subtitleTextStyle">@style/TextAppearance.Sherlock.Widget.ActionBar.Subtitle</item>
        <item name="background">@color/red_ab_zdf</item>
        <item name="android:background">@color/red_ab_zdf</item>
        <item name="backgroundStacked">@color/red_ab_zdf</item>
        <item name="android:backgroundStacked">@color/red_zdf</item>
        <item name="divider">?attr/dividerVertical</item>
        <item name="progressBarStyle">@style/Widget.Sherlock.Light.ProgressBar.Horizontal</item>
        <item name="indeterminateProgressStyle">@style/Widget.Sherlock.Light.ProgressBar</item>
        <item name="progressBarPadding">32dip</item>
        <item name="itemPadding">8dip</item>
    </style>

    <style name="CustomAbsOverlayStyle" parent="Widget.Sherlock.ActionBar.Solid"></style>


    <style name="loading_small" parent="@android:style/Widget.ProgressBar.Small.Inverse">
        <item name="android:layout_width">21dp</item>
        <item name="android:layout_height">21dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    
     <style name="shadow_above" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/img_shadow_above</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    
     <style name="shadow_below" >
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/img_shadow_below</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="tv_normal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/textSize_3</item>
        <item name="android:textColor">@color/black_tv_zdf</item>
        <item name="android:textColorHint">@color/et_hint</item>
        <item name="android:textCursorDrawable">@drawable/img_ed_cursor</item>
    </style>

    <style name="tv_small" parent="tv_normal">
